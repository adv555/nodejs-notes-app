            "type": "string",
            "example": "123456789"
          },
          "name": {
            "type": "string",
            "example": "New Feature"
          },
          "content": {
            "type": "string",
            "example": "deadline 05/06/2021"
          },
          "category": {
            "type": "string",
            "example": "Idea"
          },
          "date": {
            "type": "array",
            "items": {
              "type": "string",
              "example": "05/06/2021"
            }
          },
          "archived": {
            "type": "boolean"
          },
          "createdAt": {
            "type": "string",
            "format": "data-time",
            "example": "2016-08-29T09:12:33.001+0000"
          },
          "updatedAt": {
            "type": "string",
            "format": "data-time",
            "example": "2016-08-29T09:12:33.001+0000"
          }
        }
      },
      "NewNote": {
        "required": [
          "category",
          "content",
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
